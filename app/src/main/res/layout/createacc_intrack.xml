<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/img_BG"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/bg_intrack" />

    <Button
        android:id="@+id/btn_CreateAcc"
        android:layout_width="854px"
        android:layout_height="139px"
        android:background="@drawable/shape_styles"
        android:backgroundTint="#DE703C"
        android:fontFamily="@font/nunito_sans_bold"
        android:text="CREATE ACCOUNT"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword"
        app:layout_constraintVertical_bias="0.284" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="854px"
        android:layout_height="133px"
        android:background="@drawable/shape_styles"
        android:ems="10"
        android:fontFamily="@font/nunito_sans_bold"
        android:inputType="textPassword"
        android:padding="25px"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_without4"
        app:layout_constraintVertical_bias="0.102"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="854px"
        android:layout_height="133px"
        android:layout_marginBottom="92dp"
        android:background="@drawable/shape_styles"
        android:ems="15"
        android:fontFamily="@font/nunito_sans"
        android:inputType="textPersonName"
        android:padding="25px"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_without2"
        app:layout_constraintVertical_bias="0.064" />

    <TextView
        android:id="@+id/lbl_without2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_bold"
        android:text="Username"
        android:textColor="#222222"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        app:layout_constraintVertical_bias="0.047" />

    <TextView
        android:id="@+id/lbl_without3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_bold"
        android:text="Email Address"
        android:textColor="#222222"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3"
        app:layout_constraintVertical_bias="0.063" />

    <TextView
        android:id="@+id/lbl_without4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_bold"
        android:text="Password"
        android:textColor="#222222"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_EmailAdd"
        app:layout_constraintVertical_bias="0.092" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="854px"
        android:layout_height="133px"
        android:layout_marginBottom="92dp"
        android:background="@drawable/shape_styles"
        android:ems="15"
        android:fontFamily="@font/nunito_sans"
        android:inputType="textPersonName"
        android:padding="25px"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_without"
        app:layout_constraintVertical_bias="0.032" />

    <TextView
        android:id="@+id/lbl_without"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_sans_bold"
        android:text="Name"
        android:textColor="#222222"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.117"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.034" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="476px"
        android:layout_height="228.62px"
        android:src="@drawable/logo_intrack"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.089" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.238"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.039"
        app:srcCompat="@drawable/createacctext" />

    <EditText
        android:id="@+id/edit_EmailAdd"
        android:layout_width="854px"
        android:layout_height="133px"
        android:layout_marginTop="20dp"
        android:background="@drawable/shape_styles"
        android:ems="10"
        android:fontFamily="@font/nunito_sans"
        android:inputType="textEmailAddress"
        android:padding="25px"
        android:textSize="46.07px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_without3"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />
</android.support.constraint.ConstraintLayout>